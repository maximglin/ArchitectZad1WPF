<UserControl x:Class="ArchitectZad1WPF.OpenFile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ArchitectZad1WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Grid.Column="0" LastChildFill="True" Margin="5px">
        <Button DockPanel.Dock="Right" Padding="10px 0px 10px 0px" Click="Button_Click">Open</Button>
        <Button DockPanel.Dock="Right" Padding="10px 0px 10px 0px" Click="Mode_Click">Mode</Button>
        <TextBox Name="txt" DockPanel.Dock="Left" IsReadOnly="True"></TextBox>
    </DockPanel>

</UserControl>
